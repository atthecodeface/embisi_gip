#a Targets
all:

sim: simulation

doc: documentation


.PHONY: simulation

simulation:
	$(MAKE) -f Makefile.simulation

simclean:
	$(MAKE) -f Makefile.simulation clean

.PHONY: documentation

documentation:
	$(MAKE) -f Makefile.documentation

