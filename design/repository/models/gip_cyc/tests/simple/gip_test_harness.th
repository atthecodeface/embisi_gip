printf "GIP Test harness starting"

reset "test_reset" 1

wait 1

reset "test_reset" 0

drive "in" 1

wait 11

printf "Output value %d0%" input("result")
#bne input("result") 210 test_fail

pass 0 "Test succeeded"

end

test_fail:
printf "Test failed %d0% %d1%" global_cycle() input("result")
fail global_cycle() "**************************************************************************** Test failed"
end

