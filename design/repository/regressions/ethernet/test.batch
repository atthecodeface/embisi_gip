string vcd_file

setenv gip_mif "../gip/bin/test_pst.gif"
read_hw_file simple_tx.hwex

sprintf vcd_file "%s0%/test_pst.vcd" env_string("run_dir")
vcd_file_open "allsignals" vcd_file
vcd_file_add "allsignals" "mii" "etx"
vcd_file_enable "allsignals"
#global_monitor_display "mux_data" "Mux type %0x% data %1x% source %0x%" global_signal_value("mux_type") global_signal_value("mux_data") global_signal_value("mux_ack")
reset
step 1500
vcd_file_close "allsignals"

